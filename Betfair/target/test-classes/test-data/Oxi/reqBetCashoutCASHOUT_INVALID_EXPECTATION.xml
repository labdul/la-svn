<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE oxip SYSTEM "reqBetCashout.dtd">
<oxip version="7.0">
	<request>
		<reqClientAuth returnToken="N">
			<user>{user}</user>
			<password>{pass}</password>
			<ssoid>{ssoid}</ssoid>
		</reqClientAuth>
		<reqBetCashout>
        <betId>{betId}</betId>
        <cashoutValue>1.00</cashoutValue>
        <expectation>
            <outcomeId>99999</outcomeId>
            <priceType>L</priceType>
            <maxPriceNum>2</maxPriceNum>
            <maxPriceDen>1</maxPriceDen>
        </expectation>
    </reqBetCashout>
	</request>
</oxip>