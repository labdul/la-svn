<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE oxip SYSTEM "oxip.dtd">
<oxip version="6.0">
	<request>
		<reqClientAuth>
			<user>{user}</user>
			<password>{pass}</password>
			<ssoid>{ssoid}</ssoid>
		</reqClientAuth>
		<reqBetPlace>
			<token/>
			<errorDetail>ALL</errorDetail>
			<channel>I</channel>
			<fullDetails>N</fullDetails>
			<bet>
				<betNo>1</betNo>
				<stakePerLine>5</stakePerLine>
				<betType>SGL</betType>
				<legType>W</legType>
				<leg>
					<legNo>1</legNo>
					<legSort>--</legSort>
					<part>
						<partNo>1</partNo>
						<outcome>{outcome}</outcome>
						<priceType>L</priceType>
					</part>
				</leg>
				<freebetTokenId>{freebetTokenId}</freebetTokenId>
			</bet>
		</reqBetPlace>
	</request>
</oxip>